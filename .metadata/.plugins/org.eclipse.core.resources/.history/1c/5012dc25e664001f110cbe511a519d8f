package edu.kh.basic;

// 한줄주석 :컴파일러가 해석하지 않는 부분

/*
 * 범위주석 
 * */

// class : 자바 코드가 작성되는 영역 
public class JavaBasic {
// main 쓰고 ctrl +space -> 맨 위에 maint method자동완성 enter
	public static void main(String[] args) {
		// main method (메서드 : 자바 애플리케이션(프로그램)을 실행하기 위해
		// 반드시 필요한 메서드

		// 콘솔창에 출력 메서드
		System.out.println("hello world");
		System.out.println("아아아아아");
		System.out.println("아아아아아");
		System.out.println("아아아아아");
		//syso 치고ctrl +space -> system.out.println(); 자동완성 
		//실행 :ctrl +f11
		//한줄복사 ctrl alt 방향키 (위아래 ): 방향키 방향으로 해당 줄 복사

		System.out.println("-------------------------------------");
		//숫자 연산 
		System.out.println("1+2"); //문자열 그대로 
		System.out.println(1+2); //3  숫자연산 
		
		//""안에 작성된 코드는 단순 문자열로 인식 
		//""안에 작성되지 않은 코드는 숫자 , 변수로 인식 
		System.out.println("-------------------------------------");
		
		System.out.println(50-23);
		System.out.println(12*13);
		System.out.println(12*3);
		System.out.println(12%3);
	
	}

}